{
  "name": "node-starter-es6",
  "version": "1.0.0",
  "description": "NodeJS Starter Repository With ES6",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "del /s /q dist && babel server.js --out-dir dist",
    "dev": "babel-watch server.js",
    "prelint:js": "npm run format:all",
    "lint:js": "eslint ./*.js",
    "format:all": "prettier --write *.js"
  },
  "keywords": [
    "NodeJS",
    "Express",
    "ES6",
    "Starter Repository"
  ],
  "author": "Burhan Bharmal",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.15.3"
  }
}
